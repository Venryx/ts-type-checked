{
	"compilerOptions": {
		"module": "CommonJS",
		"allowJs": true,
		"target": "es2017",
		"lib": ["es2017"],
		"outDir": "dist",
		"noImplicitAny": true,
		"noUnusedLocals": true,
		"removeComments": true,
		"experimentalDecorators": true,
		"moduleResolution": "node",
		"strictNullChecks": true,
		"plugins": [{ "transform": "ts-type-checked/transformer", "logLevel": "debug", "nullIsUndefined": true }]
	}
}
