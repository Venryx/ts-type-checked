{
  "name": "ts-type-checked",
  "version": "0.5.0",
  "description": "Type checking utilities for TypeScript.",
  "main": "index.js",
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/janjakubnanista/ts-type-checked.git"
  },
  "keywords": [
    "TypeScript",
    "type checking",
    "type checks",
    "duck typing",
    "type guards"
  ],
  "author": "Jan Jakub Nanista <jan.jakub.nanista@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/janjakubnanista/ts-type-checked/issues"
  },
  "homepage": "https://github.com/janjakubnanista/ts-type-checked#readme",
  "peerDependencies": {
    "typescript": "^2.7.2"
  },
  "scripts": {
    "build": "yarn build:compile && yarn build:lint && yarn build:meta",
    "build:compile": "tsc",
    "build:lint": "eslint ./dist --fix --ext .js,.ts",
    "build:meta": "./scripts/copy-metadata.sh ../ ./dist",
    "clean": "rm -rf dist",
    "test": "echo 'No test command defined'",
    "watch": "tsc --watch"
  }
}
