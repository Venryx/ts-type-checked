{
  "name": "ts-type-checked",
  "version": "0.0.1",
  "description": "Runtime duck type checking utilities for TypeScript.",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf transformer/**/*.{js,d.ts}",
    "lint": "eslint . --fix --ext .ts",
    "prepublish": "yarn build",
    "test": "./scripts/test-all-versions.sh",
    "test:version": "./scripts/test.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/janjakubnanista/ts-type-checked.git"
  },
  "keywords": [
    "TypeScript",
    "type checking",
    "duck typing"
  ],
  "author": "Jan Jakub Nanista <jan.jakub.nanista@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/janjakubnanista/ts-type-checked/issues"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "transformer/**/*.js",
    "transformer/**/*.d.ts"
  ],
  "homepage": "https://github.com/janjakubnanista/ts-type-checked#readme",
  "devDependencies": {
    "@types/jest": "^25.1.3",
    "@types/node": "^12.12.12",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.5.0",
    "fast-check": "^1.22.2",
    "husky": "^3.0.9",
    "jest": "^25.1.0",
    "lint-staged": "^9.4.2",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.1",
    "tslib": "^1.10.0",
    "ttypescript": "^1.5.10",
    "typescript": "^3.7.2"
  },
  "peerDependencies": {
    "typescript": "^3.7.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "preset": "ts-jest",
    "globals": {
      "ts-jest": {
        "compiler": "ttypescript",
        "tsConfig": "tsconfig.test.json"
      }
    }
  },
  "lint-staged": {
    "*.{js,ts}(x)?": [
      "eslint --fix",
      "git add"
    ]
  }
}
